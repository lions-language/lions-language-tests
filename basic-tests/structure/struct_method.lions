struct DateTime {
	year: u16,
	month: u8,
	day: u8
}

struct Cert {
	no: &string,
	create_time: DateTime
}

struct UserInfo {
	name: &string,
	age: u8,
	cert: Cert
}
func [this: &UserInfo] get_cert() -> &Cert {
	return &this.cert
}

func test_struct_member_call() {
	var user_info = UserInfo {
		name: "Mike",
		cert: Cert {
			no: "123456",
			create_time: DateTime{
				month: 12,
				year: 2020,
				day: 12
			}
		},
		age: 20
	}
	var cert = &user_info.get_cert()
	println(cert.no)
}

func main() {
	test_struct_member_call()
}

